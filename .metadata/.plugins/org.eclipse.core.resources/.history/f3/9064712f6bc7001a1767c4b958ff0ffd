import java.util.Random;
import java.util.Scanner;

public class Coupon {
	
	
public static void main(String[] args) {
	//System.out.println("Enter the Number of Coupons You Want");
	//Scanner sc = new Scanner(System.in);
	//int num = sc.nextInt();
	int result = getCoupon();
	System.out.println(result);
	
}

private static int getCoupon() {
	Random r = new Random();
	/*The nextInt(n) method returns a random integer in the range 0 to n-1 inclusive. 
	 * So the lowest possible value of num is 0 + 1000000 = 1000000, 
	 * and the highest possible value is 9000000 - 1 + 1000000 = 9999999, as desired.
	 */
	int newRandomNum = r.nextInt(90000000)+1000000;
	return newRandomNum;
}
}
